{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "rootDir": "../",
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": [
      "ts-jest",
      {
        "tsconfig": "tsconfig.json"
      }
    ]
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  },
  "modulePaths": [
    "<rootDir>"
  ]
}